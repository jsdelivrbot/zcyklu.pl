<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

<div class="container nav-wrapper">
  <% include ../partials/nav.ejs %>
</div>      <!-- /.nav-wrapper -->

<main class="container about-us-wrapper">
<%- page.content %>
<div class="authors-wrapper col-lg-12">
    <h1>
        Redakcja
    </h1>
    <% page.authors.forEach(function (author) { %>
    <a href="#" class="author-biogram row">
      <img class="col-lg-2" src="<%= author.fields.profilePhoto.fields.file.url %>?w=125">
      <p class="col-lg-10">
         <%= author.fields.name %>
      </p>
    </a>
    <% }); %>
</div>
</main> <!-- /container -->

<% include ../partials/footer.ejs %>

</body>
</html>
